{
  "extends": "eslint:recommended",
  "rules": {
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "object-curly-spacing": [2, "always"]
  },
  "plugins": ["jest"],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "root": true
}